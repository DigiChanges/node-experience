    "pre-commit": "lint-staged",
    "pre-check": "pnpm ts-check && pnpm lint",
    "prepare": "husky",
    "postinstall": "node_modules/.bin/deepkit-type-install",
    "start": "node dist/src/index.js",
    "test-ci": "jest --run-in-band --coverage",
