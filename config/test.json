{
  "env": "test",
  "auth": {
    "authorization": true
  },
  "apiWhitelist": [
    {
      "methods": ["GET"],
      "url": "/"
    },
    {
      "methods": ["POST"],
      "url": "/api/auth/login"
    },
    {
      "methods": ["POST"],
      "url": "/api/auth/signup"
    },
    {
      "methods": ["POST"],
      "url": "/api/auth/forgot-password"
    },
    {
      "methods": ["POST"],
      "url": "/api/auth/change-forgot-password"
    },
    {
      "methods": ["PUT"],
      "url": "/api/auth/verify-your-account/**"
    },
    {
      "methods": ["PUT"],
      "url": "/api/auth/verify-your-account/**"
    },
    {
      "methods": ["POST"],
      "url": "/api/auth/refresh-token"
    },
    {
      "methods": ["*"],
      "url": "/test/all/*"
    },
    {
      "methods": ["GET"],
      "url": "/test/countries"
    },
    {
      "methods": ["GET"],
      "url": "/test/countries/**"
    },
    {
      "methods": ["GET"],
      "url": "/test/countries/**/states"
    },
    {
      "methods": ["GET"],
      "url": "/test/countries/**/states/**/cities"
    },
    {
      "methods": ["GET"],
      "url": "/test/countries/**/states/*"
    }
  ]
}
