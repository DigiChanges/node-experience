{
  "env": "NODE_ENV",
  "serverPort": "SERVER_PORT",
    "auth": {
      "authorization": "AUTHORIZATION"
  },
  "dbConfig": {
    "TypeORM": {
      "host": "DB_HOST",
      "port": "DB_PORT",
      "username": "DB_USER",
      "password": "DB_PASSWORD",
      "database": "DB_DATABASE"
    },
    "Mongoose": {
      "host": "DB_HOST",
      "port": "DB_PORT",
      "database": "DB_USER",
      "username": "DB_PASSWORD",
      "password": "DB_DATABASE"
    },
    "default" : "DB_TYPE_DEFAULT"
